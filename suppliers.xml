<duke>
  <schema>
    <threshold>0.85</threshold>
    <path>test</path>
    
    <property type="id">
      <name>ID</name>
    </property>

    <property>
      <name>NAME</name>
      <comparator>no.priv.garshol.duke.ExactComparator</comparator>
      <low>0.48</low>
      <high>0.9</high>
    </property>
    
    <property>
      <name>ASSOCIATION_NO</name>
      <comparator>no.priv.garshol.duke.ExactComparator</comparator>
      <low>0.1</low>
      <high>0.88</high>
    </property>

    <property>
      <name>COUNTRY_DB</name>
      <comparator>no.priv.garshol.duke.ExactComparator</comparator>
      <low>0.1</low>
      <high>0.5</high>
    </property>

    <property>
      <name>ADDRESS1</name>
      <comparator>no.priv.garshol.duke.ExactComparator</comparator>
      <low>0.48</low>
      <high>0.7</high>
    </property>

    <property>
      <name>ADDRESS2</name>
      <comparator>no.priv.garshol.duke.ExactComparator</comparator>
      <low>0.48</low>
      <high>0.7</high>
    </property>

    <property>
      <name>ZIP_CODE</name>
      <comparator>no.priv.garshol.duke.ExactComparator</comparator>
      <low>0.35</low>
      <high>0.55</high>
    </property>
  </schema>
  
  <csv>
    <param name="input-file" value="suppliers-merged.csv"/>
    <!-- encoding -->
    <!-- header-line -->
    <column name="SUPPLIER_ID" property="ID" prefix="supplier/"/>
    <column name="NAME"
            cleaner="no.priv.garshol.duke.LowerCaseNormalizeCleaner"/>
    <column name="ASSOCIATION_NO"
            cleaner="no.priv.garshol.duke.AssociationNoCleaner"/>
    <column name="COUNTRY_DB"/>
    <column name="ADDRESS1"
            cleaner="no.priv.garshol.duke.LowerCaseNormalizeCleaner"/>
    <column name="ADDRESS2"
            cleaner="no.priv.garshol.duke.LowerCaseNormalizeCleaner"/>
    <column name="ZIP_CODE"
            cleaner="no.priv.garshol.duke.TrimCleaner"/>
  </csv>

  <csv>
    <param name="input-file" value="customers-merged.csv"/>
    <!-- encoding -->
    <!-- header-line -->
    <column name="CUSTOMER_ID" property="ID" prefix="customer/"/>
    <column name="NAME"
            cleaner="no.priv.garshol.duke.LowerCaseNormalizeCleaner"/>
    <column name="ASSOCIATION_NO"
            cleaner="no.priv.garshol.duke.AssociationNoCleaner"/>
    <column name="COUNTRY_DB"/>
    <column name="ADDRESS1"
            cleaner="no.priv.garshol.duke.LowerCaseNormalizeCleaner"/>
    <column name="ADDRESS2"
            cleaner="no.priv.garshol.duke.LowerCaseNormalizeCleaner"/>
    <column name="ZIP_CODE"
            cleaner="no.priv.garshol.duke.TrimCleaner"/>
  </csv>
</duke>